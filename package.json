{
  "name": "superbacked",
  "productName": "Superbacked",
  "version": "1.9.0",
  "description": "Superbacked is a secret management platform used to back up and pass on sensitive data from one generation to the next.",
  "main": ".webpack/main",
  "scripts": {
    "code": "export ENV=development && electron-forge start",
    "lint": "eslint --ext .ts,.tsx .",
    "prebuild": "export ENV=production && electron-forge make",
    "build": "npm run build:darwin:arm64 && npm run build:darwin:x64 && npm run build:linux:arm64 && npm run build:linux:x64",
    "build:darwin:arm64": "APPLE_KEYCHAIN_PROFILE=superbacked-notarytool electron-builder --config ./builder.config.ts --mac --arm64",
    "build:darwin:x64": "APPLE_KEYCHAIN_PROFILE=superbacked-notarytool electron-builder --config ./builder.config.ts --mac --x64",
    "build:linux:arm64": "electron-builder --config ./builder.config.ts --linux --arm64",
    "build:linux:x64": "electron-builder --config ./builder.config.ts --linux --x64",
    "package": "./package.sh",
    "sign-dependencies": "find ./{bin,superbacked-os/*.img,superbacked-os-utilities/sedutil-cli} -type f -print0 | sort -z | xargs -0 shasum --algorithm 256 > SHA256SUMS && gpg --armor --detach-sig --output SHA256SUMS.asc SHA256SUMS",
    "sign-release": "cd dist && find . -name 'superbacked-*' -type f -print0 | sort -z | xargs -0 shasum --algorithm 256 > SHA256SUMS && gpg --armor --detach-sig --output SHA256SUMS.asc SHA256SUMS && cd -",
    "ncu": "ncu --reject @vercel/webpack-asset-relocator-loader --target minor --upgrade"
  },
  "author": {
    "name": "Sun Knudsen",
    "email": "hello@sunknudsen.com"
  },
  "license": "UNLICENSED",
  "private": true,
  "devDependencies": {
    "@electron-forge/cli": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-webpack": "^7.10.2",
    "@electron/notarize": "^3.1.1",
    "@eslint/compat": "^1.4.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.39.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.6.1",
    "@svgr/webpack": "^8.1.0",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vercel/webpack-asset-relocator-loader": "1.10.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.1",
    "electron": "^38.7.0",
    "electron-builder": "^26.2.0",
    "eslint": "^9.39.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^9.1.0",
    "fs-extra": "^11.3.2",
    "globals": "^16.5.0",
    "node-loader": "^2.1.0",
    "npm-check-updates": "^19.1.2",
    "prettier": "^3.6.2",
    "react-refresh": "^0.18.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/serialize": "^1.3.3",
    "@emotion/styled": "^11.14.1",
    "@emotion/utils": "^1.4.2",
    "@fontsource/roboto-mono": "^5.2.8",
    "@mantine/core": "^8.3.8",
    "@mantine/emotion": "^8.3.8",
    "@mantine/form": "^8.3.8",
    "@mantine/hooks": "^8.3.8",
    "@scure/bip39": "^2.0.1",
    "@sunknudsen/totp": "^1.2.0",
    "@zxcvbn-ts/core": "^3.0.4",
    "@zxcvbn-ts/language-common": "^3.0.4",
    "@zxcvbn-ts/language-en": "^3.0.2",
    "blockcrypt": "^0.0.1-beta.22",
    "commander": "^14.0.2",
    "electron-store": "^8.2.0",
    "leven": "^4.1.0",
    "pdfjs-dist": "^4.10.38",
    "qr": "^0.5.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.3",
    "react-router-dom": "^7.9.6",
    "tabler-icons-react": "^1.56.0"
  },
  "prettier": {
    "endOfLine": "lf",
    "printWidth": 80,
    "semi": false,
    "tabWidth": 2,
    "trailingComma": "es5"
  }
}
